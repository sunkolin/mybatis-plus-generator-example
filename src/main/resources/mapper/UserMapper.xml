<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.starfish.mapper.UserMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.starfish.entity.UserEntity">
        <id column="id" property="id" />
        <result column="mobile" property="mobile" />
        <result column="password" property="password" />
        <result column="nick_name" property="nickName" />
        <result column="user_name" property="userName" />
        <result column="full_name" property="fullName" />
        <result column="email" property="email" />
        <result column="sex" property="sex" />
        <result column="birthday" property="birthday" />
        <result column="remark" property="remark" />
        <result column="create_time" property="createTime" />
        <result column="modify_time" property="modifyTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, mobile, password, nick_name, user_name, full_name, email, sex, birthday, remark, create_time, modify_time
    </sql>

</mapper>
